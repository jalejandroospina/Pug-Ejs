
<div class="container mt-5">
  <div>
     <div class="mt-5"><h2>Productos Agregados</h2></div>
         <table class="table mt-5">
            <thead>
              <tr>      
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Imagen</th>
               </tr>
            </thead>

            <tbody>
                <tr>
                    {{#each productos}}
                <tr>
                            
                  <td>{{nombre}} </td>
                  <td>{{precio}} </td>
                  <td> <img src={{imagen}} alt="image" width="50px" height="50px"> </td>
                </tr>
                    {{/each}}
                </tr>
      
             </tbody>
        </table>
      </div>
</div>